    "next-auth": "^4.18.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
