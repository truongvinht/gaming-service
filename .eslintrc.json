{
  "env": {
      "node": true,
      "es6": true,
      "jest": true
  },
  "extends": [
      "standard",
      "next/core-web-vitals"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "indent":[
          "error",
          4
      ],
      "semi": [
          "error",
          "always"
      ]
  }
}